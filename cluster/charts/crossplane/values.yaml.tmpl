replicas: 1

deploymentStrategy: RollingUpdate

image:
  repository: crossplane/crossplane
  tag: %%VERSION%%
  pullPolicy: Always

args:
- '--debug'

imagePullSecrets:
- dockerhub

personas:
  deploy: true

clusterPackages:
  aws:
    version: v0.10.0
    deploy: false
  gcp:
    version: v0.10.0
    deploy: false
  azure:
    version: v0.10.0
    deploy: false
  rook:
    version: v0.7.0
    deploy: false

hostedConfig:
  enabled: false

templateStacks:
  enabled: true
  controllerImage: crossplane/templating-controller:v0.4.0

priorityClassName: ""

resourcesCrossplane:
  limits:
    cpu: 100m
    memory: 512Mi
  requests:
    cpu: 100m
    memory: 256Mi

resourcesPackageManager:
  limits:
    cpu: 100m
    memory: 512Mi
  requests:
    cpu: 100m
    memory: 256Mi

forceImagePullPolicy: ""

insecureAllowAllApigroups: false

insecurePassFullDeployment: false

insecureInstallJob: false
